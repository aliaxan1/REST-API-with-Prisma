// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Student {
roll_no        Int    @id @default(autoincrement())
  first_name     String   
  last_name      String?
  email         String?   
}

